@{
    ViewData["Title"] = "Print";
}
<h1>Print Details</h1>

<table>
    <tr>
        <td>Id</td>
        <td>
            @ViewBag.Id
        </td>
    </tr>
    <tr>
        <td>Name</td>
        <td>
            @ViewBag.Name
        </td>
    </tr>
    <tr>
        <td>Salary</td>
        <td>
            @ViewBag.Salary
        </td>
    </tr>
    <tr>
        <td>Department</td>
        <td>
            @ViewBag.Department
        </td>
    </tr>
    <tr>
        <td>Gender</td>
        <td>
            @ViewBag.Gender
        </td>
    </tr>
</table>